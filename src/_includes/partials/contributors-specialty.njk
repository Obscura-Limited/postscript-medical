{% set header = contributorsHeader %}
{% set subheader = contributorsSubHeader %}
{% set contributors = contributorsContent %}

<h2>
  {{ header }}
</h2>

<p>
  {{ subheader }}
</p>

<ul class="contributor__list stacker--large">
  {% for contributor in contributors %}
    <li class="contributor">
      {% set avatarContent = contributor.data %}
      {% include "partials/contributor-avatar.njk" %}
    </li>
  {% endfor %}
</ul>